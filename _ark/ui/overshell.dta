{new
    UIPanel
    overshell_angle_panel
    (file
        "practice/practice_sel_section_og.milo")
    (focus sel_section.lst)
    (enter
        {sel_section.lst set_data
            (rb2 rb3 rb4 rr)}
        {title.lbl set_localized {localize os_angle}})
